# Stonebranch 2018
# Task to Monitor for SAP Events (SM62)
# use standard SAP XBP Interface function to read and confirm SAP Events 
#
#
# Version 1.0, 10.08.2017, Nils Buer
#         2.0. 13.06.2017, Nils Buer update for dest connection
#         2.1  20.06.2018, Nils Buer update adjusted to CTK
#         2.2  21.06.2018, Nils Buer task action added
#         2.3. 26.06.2018, Nils Buer functions added
#         2.4. 27.06.2018, Nils Buer wildcard use *
#         2.5. 28.06.2018, Nils Buer advanced error handling added 
#
# Description: 
# Queries the SAP Event history table for the selected SAP Event &amp; Parameter
# If the Event is found, it gets confirmed, so that it is not triggered again
#


# Functions

checkerror () {

# checks is usap returns a non zero exit code e.g. saplogon error 202

local exitcode="$1"
local calledfunction="$2"
local status="$3"
local task_name="$4"

if [ "$exitcode" -ne 0 ]; then
   printf "##ERROR# ###in function $calledfunction exitcode =$exitcode \n" 1&gt;&amp;2
   exit 1
fi

# test additional error message that do not throw an error code

if [[ $status = *"No tasks found matching"* ]]; then
      printf "##ERROR#in function $calledfunction No tasks found matching with name: $task_name \n"  1&gt;&amp;2
      exit 1
fi

}



print_input_variables () {

# Input Variables

printf "##INFO# ### USAP started with loglevel: ${ops_var_level} \n"
printf "##INFO# ### exec. USAP from: ${ops_var_usapdir} \n"
printf "##INFO# ### SAP destination: ${ops_var_ashost} \n"
printf "##INFO# ### SAP Client: ${ops_var_client} \n"
printf "##INFO# ### SAP User: ${_credentialUser("${ops_var_sapcred}")} \n"
printf "##INFO# ### SAP Password: ${_credentialPwd("${ops_var_sapcred}")} \n"
printf "##INFO# ### Event ID: ${ops_var_event_id} \n"
printf "##INFO# ### Event Parameter: ${ops_var_event_para} \n"
printf "##INFO# ### Select State: ${ops_var_select_state} \n"
printf "##INFO# ### Event Creation: ${ops_var_creation_action} \n"
printf "##INFO# ### Poll Intervall: ${ops_var_poll} \n"
printf "##INFO# ### OMS host: ${ops_var_omshost} \n"
printf "##INFO# ### OMS Port: ${ops_var_omsport} \n"
printf "##INFO# ### OMS User: ${_credentialUser("${ops_var_omscred}")} \n"
printf "##INFO# ### OMS Password: ${_credentialPwd("${ops_var_omscred}")} \n"
printf "##INFO# ### OPS cli dir: ${ops_var_opsclidir} \n"
printf '##INFO# ### Task to launch: ${ops_var_taskname} \n'
printf " \n"
printf " \n"
printf "##INFO# ### Monitoring for SAP EVENT:${ops_var_event_id} and Parameter:${ops_var_event_para} \n"

}

test_dir () {

# checks if a directory exists

if [ -d "$1" ]; then 
    printf "##INFO# $1 exists. \n"
else 
    printf "##ERROR# $1 does not exists. \n"  1&gt;&amp;2
    exit 1
fi
}

check_task_status () {

# checks if the Universal Task is running and has not been forced finished

local task_name="$1"
local status=""
status=$(${ops_var_opsclidir}ops-task-status -r OMS -m ${ops_var_omsport}@${ops_var_omshost} -u ${_credentialUser("${ops_var_omscred}")} -p ${_credentialPwd("${ops_var_omscred}")} task-instance-name="$task_name" task-status=running)
checkerror "$?" "check_task_status"


if [[ $status = *"RUNNING"* ]]; then
   return 0
else 
   return 1
fi
}

search_for_sap_event () {

# calls the usap command display event_history to query the sap history table

local status=""
local event_found=""
local event_parameter="${ops_var_event_para}"
# If no event parameter or * is specified in UT than set to parameter to \* for search
if [[ $event_parameter = "" ]] || [[ $event_parameter = "*" ]]; then
   status=$(usap -dest ${ops_var_ashost} -client ${ops_var_client} -userid ${_credentialUser("${ops_var_sapcred}")} -pwd ${_credentialPwd("${ops_var_sapcred}")} -level ${ops_var_level} -immediate -lang english -level info  -display event_history -event_id ${ops_var_event_id} -event_parm \* -event_select_state ${ops_var_select_state} -event_action ${ops_var_creation_action}) 
   checkerror "$?" "search_for_sap_event"
else
   status=$(usap -dest ${ops_var_ashost} -client ${ops_var_client} -userid ${_credentialUser("${ops_var_sapcred}")} -pwd ${_credentialPwd("${ops_var_sapcred}")} -level ${ops_var_level} -immediate -lang english -level info  -display event_history -event_id ${ops_var_event_id} -event_parm ${event_parameter} -event_select_state ${ops_var_select_state} -event_action ${ops_var_creation_action}) 
   checkerror "$?" "search_for_sap_event"
fi

event_found=$(echo $status | awk -F"|" '{if ($11 ~ /N[ ]/) print substr($11,1,1)}')

if [[ $event_found = "N" ]]; then
   printf "##INFO# EVENT:${ops_var_event_id} and Parameter:${ops_var_event_para} in STATUS $event_found found. \n"
   return 0
else 
   return 1
fi
}        

launch_task () {

# calls the cli command ops-task-launch to launch the specified task in the UT

local task_name="$1"
local status=""

if [[ $task_name = "" ]]; then
   printf "##INFO# Monitoring Mode, No task to launch specified \n"
else
   status=$(${ops_var_opsclidir}ops-task-launch -r OMS -m ${ops_var_omsport}@${ops_var_omshost} -u ${_credentialUser("${ops_var_omscred}")} -p ${_credentialPwd("${ops_var_omscred}")} task-name="$task_name")
   checkerror "$?" "launch_task" "$status" "$task_name"
   printf "##INFO# Task launched: $task_name \n"
fi
}


### Main

printf "##INFO# Directories \n"
test_dir ${ops_var_usapdir}
test_dir ${ops_var_opsclidir}
printf "##INFO# Settings \n"
print_input_variables


while [ 1 ]
do
   # test if universal task is still running
   check_task_status '${ops_task_name}'
   if [ "$?" -eq 0 ]; then
      search_for_sap_event
      if [ "$?" -eq 0 ]; then
         launch_task '${ops_var_taskname}'
         exit 0 
      fi
   else
      printf "##ERROR# UT is not running \n" 1&gt;&amp;2
      exit 1
   fi
   sleep  ${ops_var_poll}
done
